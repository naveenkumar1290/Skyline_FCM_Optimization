<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"



 android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".old_activity.Damagerefushchart" >
          <ScrollView 
          android:layout_height="match_parent"
            android:layout_width="match_parent">
<RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
  
            
   
        <RelativeLayout 
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:id="@+id/name">

        <LinearLayout
            android:id="@+id/linerfortime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3" >

            <ImageView
                android:id="@+id/merchantname"
                android:layout_width="100sp"
                android:layout_height="40sp"
         		android:layout_gravity="right" 
                android:layout_weight="2" 
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="-10sp"
       
                  />

            <TextView
                android:id="@+id/timer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/merchantname"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/merchantname"
                android:layout_weight="1"
                android:gravity="right"
                android:lines="2"
                android:maxLines="2"
                android:text="Time Clock 00:00"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="@dimen/clientname" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/eusername"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/name"
        android:background="@drawable/gradientimage" >

        <ImageView
            android:id="@+id/missing"
            android:layout_width="40sp"
            android:layout_height="20sp"

           
            android:gravity="center"
            android:text="Missing Crates "
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            
android:layout_toRightOf="@+id/missing"
            android:layout_centerVertical="true"
            android:text="Hallieburtonâ€™s Crates"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="@dimen/clientname" />
    </RelativeLayout>
      <LinearLayout
        android:id="@+id/jobdeta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:layout_marginTop="10sp"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/eusername" >
       

           <TextView
               android:id="@+id/jobid"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
               android:gravity="left"
               android:layout_weight="1"
               android:textSize="@dimen/createhometest"
               android:text="TextView" />

           <TextView
               android:id="@+id/projmgt"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
               android:gravity="center"
               android:layout_weight="1"
                    android:textSize="@dimen/createhometest"
               android:text="TextView" />

           <TextView
               android:id="@+id/date"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
   android:layout_gravity="right"
   android:layout_weight="1"
   android:gravity="right"
        android:textSize="@dimen/createhometest"
               android:text="TextView" />

</LinearLayout>

      

      
         <LinearLayout
        android:id="@+id/salesdata"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/jobdeta"
        android:layout_marginTop="10sp" >
       

           <TextView
               android:id="@+id/salesrap"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
               android:gravity="left"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1"
               android:text="TextView" />

           <TextView
               android:id="@+id/tech"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
               android:gravity="center"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1"
               android:text="TextView" />

           <TextView
               android:id="@+id/dated"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
   android:layout_gravity="right"
   android:layout_weight="1"
   android:gravity="right"
        android:textSize="@dimen/createhometest"
               android:text="TextView" />
</LinearLayout>
           
            <LinearLayout
        android:id="@+id/showname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/salesdata"
        android:layout_marginTop="10sp" >
       

           <TextView
               android:id="@+id/shownames"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
               android:gravity="left"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1"
               android:text="TextView" />

           <TextView
               android:id="@+id/exibit"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
               android:gravity="center"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1"
               android:text="TextView" />

           <TextView
               android:id="@+id/datedd"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
   android:layout_gravity="right"
   android:layout_weight="1"
   android:gravity="right"
        android:textSize="@dimen/createhometest"
   android:visibility="invisible"
               android:text="TextView" />
           
</LinearLayout>

              <RelativeLayout
     android:id="@+id/packglist"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:layout_marginTop="10sp"
     android:background="@color/grey"
   android:layout_below="@+id/showname">
    

     <TextView
         android:id="@+id/packglisttext"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_centerVertical="true"
         

         android:text="Confirm Crates in Bin "
         android:textSize="@dimen/clientname"
         android:textAppearance="?android:attr/textAppearanceMedium" />

    </RelativeLayout>
    
      <LinearLayout
        android:id="@+id/qnty"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/packglist"
        android:layout_marginTop="10sp" >
       

           <TextView
               android:id="@+id/qntyreq"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
               android:gravity="left"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1.5"
               android:text="TextView" />

           <TextView
               android:id="@+id/rentalorowned"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
                android:textSize="@dimen/createhometest"
               android:gravity="left"
               android:layout_weight="1.5"
               android:text="TextView" />

          
           
</LinearLayout>

        <LinearLayout
        android:id="@+id/wlh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/qnty"
        android:layout_marginTop="10sp" >
       

           <TextView
               android:id="@+id/wlhh"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
               android:gravity="left"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1.5"
               android:text="TextView" />

           <TextView
               android:id="@+id/descrip"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
               android:gravity="left"
                    android:textSize="@dimen/createhometest"
               android:layout_weight="1.5"
               android:text="TextView" />

          
           
</LinearLayout>

        
        
        
        
                <LinearLayout
        android:id="@+id/qntpack"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/wlh"
        android:layout_marginTop="10sp" >
       

           <TextView
               android:id="@+id/quntpacked"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
               android:gravity="left"
               android:layout_weight="1.5"
                    android:textSize="@dimen/createhometest"
               android:text="TextView" />

           <TextView
               android:id="@+id/casee"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
                android:textSize="@dimen/createhometest"
               android:gravity="left"
               android:layout_weight="1.5"
               android:text="TextView" />

          
           
</LinearLayout>

                
                  <LinearLayout
        android:id="@+id/pmamchech"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
android:weightSum="3"
android:background="@drawable/gradientimage"
        android:layout_below="@+id/qntpack"
        android:layout_marginTop="10sp" >
       

           <TextView
               android:id="@+id/pmamchecked"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_gravity="left"
                    android:textSize="@dimen/createhometest"
               android:gravity="left"
               android:layout_weight="1.5"
               android:text="TextView" />

           <TextView
               android:id="@+id/quantyam"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
           android:layout_gravity="center"
                android:textSize="@dimen/createhometest"
               android:gravity="left"
               android:layout_weight="1.5"
               android:text="TextView" />

          
           
</LinearLayout>
  <LinearLayout
     android:id="@+id/finifhh"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:layout_marginTop="10sp"
     android:background="@color/grey"
     android:gravity="center"
   android:layout_below="@+id/pmamchech">
    

     <TextView

         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         
         android:layout_centerVertical="true"
         android:layout_below="@+id/pmamchech"
         android:text="Finished with list "
         android:textSize="@dimen/clientname"
         android:gravity="center"
         android:textAppearance="?android:attr/textAppearanceMedium" />

    </LinearLayout>
    
</RelativeLayout>
     </ScrollView>
</RelativeLayout>